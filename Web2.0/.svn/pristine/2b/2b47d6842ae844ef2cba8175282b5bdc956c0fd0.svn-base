.clearfix {
  *zoom: 1;
}
.clearfix:before,
.clearfix:after {
  display: table;
  content: "";
  line-height: 0;
}
.clearfix:after {
  clear: both;
}
.hide-text {
  font: 0/0 a;
  color: transparent;
  text-shadow: none;
  background-color: transparent;
  border: 0;
}
.input-block-level {
  display: block;
  width: 100%;
  min-height: 30px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
/* @group flipster Navigation */
.flipster .flipster-nav {
  display: block;
  background-color: #0080c9;
  height: 40px;  /*20*/
  font-size: 0.0px;
  padding: 0px 0px 0;
  text-align: justify;
  margin-bottom: 37px;
  position: relative;
  list-style-type: none;
}
.flipster .flipster-nav:after {
  content: '';
  display: inline-block;
  width: 100%;
}
.flipster .flipster-nav li {
  display: inline-block;
  text-align: center;
  margin: 0;
  padding: 0;
  list-style-type: none;
  height: 100%;
}
.flipster .flipster-nav .flip-nav-category,
.flipster .flipster-nav .no-category {
  width: 113px;
}
.flipster .flipster-nav .flip-nav-category-link,
.flipster .flipster-nav .flip-nav-item-link {
  display: block;
  -ms-transition: all 100ms ease;
  -webkit-transition: all 100ms ease;
  -moz-transition: all 100ms ease;
  -o-transition: all 100ms ease;
  transition: all 100ms ease;
}
.flipster .flipster-nav .flip-nav-category-link,
.flipster .flipster-nav .no-category .flip-nav-item-link {
  height: 20px;
  line-height: 20px;
  color: #ffffff;
  font-size: 20px;
  padding: 10px;
}
.flipster .flipster-nav .flip-nav-current .flip-nav-category-link,
.flipster .flipster-nav .no-category .flip-nav-current.flip-nav-item-link {
  border-color: #0000ff;
  border-bottom: none;
  background: #ffffff;
  color: #000000;
}
.flipster .flipster-nav .flip-nav-category .flip-nav-items {
  display: none;
  position: absolute;
  top: 27px;
  left: 0;
  right: 0;
  text-align: center;
  padding: 10px 0;
  height: 32px;
}
.flipster .flipster-nav .flip-nav-category .flip-nav-items:after {
  content: '';
  display: inline-block;
  width: 100%;
}
.flipster .flipster-nav .flip-nav-category .flip-nav-items .flip-nav-item-link {
  padding: 5px 15px;
  color: #0000ff;
  font-family: Helvetica;
  font-size: 16px;
}
.flipster .flipster-nav .flip-nav-category .flip-nav-items .flip-nav-item-link.flip-nav-current {
  background-image: -webkit-linear-gradient(top, #FFF 0%, #0000a0 100%);
  color: #FFF;
}
.flipster .flipster-nav .flip-nav-category.flip-nav-current .flip-nav-items {
  display: block;
}
/* @end */
/* @group Previous & Next Buttons */
.flipster .flipto-prev,
.flipster .flipto-next {
  position: absolute;
  margin-top: 545px;
  z-index: 999;
  display: block;
  padding: 10px;
  margin-top: 100px;
  background-color: #0080c0;
  color: #FFF;
  border: solid 2px #0000a0;
  border-radius: 5px;
  -ms-transition: background-color 300ms ease;
  -webkit-transition: background-color 300ms ease;
  -moz-transition: background-color 300ms ease;
  -o-transition: background-color 300ms ease;
  transition: background-color 300ms ease;
  width: 75px;
  height: 40px;
  text-align: center;
  line-height: 40px;
  font-size: 20px;
}

.flipster .flipto-prev:hover,
.flipster .flipto-next:hover {
  background-color: #FFF;
  color: #0000a0;
}
.flipster .flipto-prev {
  margin-left: 113px;
}
.flipster .flipto-next {
  margin-right: 113px;
}
/* @end */